{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "cors": {
      "origin": ["https://trk-murex.vercel.app"],
      "methods": ["POST", "GET", "OPTIONS", "PUT", "DELETE"],
      "allowedHeaders": ["Origin", "X-Requested-With", "Content-Type", "Accept"],
      "credentials": true,
      "exposedHeaders": ["Access-Control-Allow-Origin"]
    }
  }